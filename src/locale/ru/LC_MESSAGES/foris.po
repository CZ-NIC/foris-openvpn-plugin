msgstr "включить"

msgid "disable"
msgstr "деактивировать"

msgid "The client certificate was successfully revoked."
msgstr "Клиентский сертификат был успешно аннулирован."

msgid "Failed to revoke the client certificate."
msgstr "Не удалось отозвать клиентский сертификат."

msgid "Unable to get OpenVPN client config."
msgstr "Не удалось получить настройки клиента OpenVPN."

msgid "Started to generate CA for the OpenVPN server."
msgstr "Началось создание центра сертификации для сервера OpenVPN."

msgid "Failed to generate CA for the OpenVPN server."
msgstr "Не удалось создать ЦС для сервера OpenVPN."

msgid "Started to generate client certificate for the OpenVPN server."
msgstr "Началось создание клиентского сертификата для сервера OpenVPN."

msgid "The OpenVPN CA was successfully deleted."
msgstr "ЦС OpenVPN был успешно удалён."

msgid "Failed to delete the OpenVPN CA."
msgstr "Не удалось удалить ЦС OpenVPN."

msgid "Router address"
msgstr "Адрес маршрутизатора"

msgid "A server address which will be present in the client config."
msgstr "Адрес сервера, который будет присутствовать в конфигурации клиента."

msgid "use autodetection"
msgstr "использовать автоопределение"

